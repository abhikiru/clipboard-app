{
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python",
            "config": { "pythonVersion": "3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url"
    }
}